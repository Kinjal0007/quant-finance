# Database
DATABASE_URL=postgresql://user:password@localhost:5432/quant_finance
USE_SQLITE=true

# GCP Configuration
GCP_PROJECT=your-project-id
GCP_REGION=europe-west3
GOOGLE_APPLICATION_CREDENTIALS=path/to/service-account.json

# BigQuery
BQ_DATASET=quant_finance
BQ_LOCATION=EU

# EOD Historical Data
EODHD_API_KEY=your-eodhd-api-key

# Twelve Data
TWELVE_DATA_API_KEY=your-twelve-data-api-key

# Feature Flags
USE_FIXTURE=false
ENABLE_AUTH=true

# Server
HOST=127.0.0.1
PORT=8080
DEBUG=true
